#pragma rtGlobals=1Macro duplicate_waves_numpnts()pduplicate_waves_numpnts()proc pduplicate_waves_numpnts(str_input,str_numpnts,str_output)string str_inputprompt str_input "input waves ?"string str_numpntsprompt str_numpnts "size restriction ?"string str_outputprompt str_output "output waves ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable/g var_numpnts = str2num(str_numpnts)silent 1pauseupdatevariable/g var_index = 0do	if(numpnts($stringfromlist(var_index,str_list)) == var_numpnts)		duplicate/o $stringfromlist(var_index,str_list) $(stringfromlist(var_index,str_list)+"_"+str_output)	endif		var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend