#pragma rtGlobals=1Macro Display_Waves_indiv_avg()pDisplay_Waves_indiv_avg()proc pDisplay_Waves_indiv_avg(str_input)string str_inputprompt str_input "input waves ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")string/g str_graph_list = sortlist(winlist("*",";","WIN:1"),";",16)string/g str_table_list = sortlist(winlist("*",";","WIN:2"),";",16)variable/g var_graph_last = itemsinlist(str_graph_list)-1string/g str_graph_last = winname(0,1)variable/g var_table_last = itemsinlist(str_table_list)-1string/g str_table_last = winname(0,2)getwindow $str_graph_last, wsizevariable/g var_left_g = v_left + 5.25variable/g var_top_g = v_top + 22.5variable/g var_right_g = v_right + 5.25variable/g var_bottom_g = v_bottom + 22.5getwindow $str_table_last, wsizevariable/g var_left_t = v_left + 5.25variable/g var_top_t = v_top + 22.5variable/g var_right_t = v_right + 5.25variable/g var_bottom_t = v_bottom + 22.5silent 1pauseupdatevariable/g var_index = 0displaydo	if(stringmatch(stringfromlist(var_index,str_list),"*_sem*") == 0)		appendtograph $stringfromlist(var_index,str_list)		if(stringmatch(stringfromlist(var_index,str_list),"*_avg*") == 1)			modifygraph mode($stringfromlist(var_index,str_list)) = 3			modifygraph marker($stringfromlist(var_index,str_list)) = 19			string/g str_sem = removeending(stringfromlist(var_index,str_list),"avg")+"sem"			errorbars/t=1.5/l=1.5 $stringfromlist(var_index,str_list) Y,wave=($str_sem,$str_sem)			modifygraph msize($stringfromlist(var_index,str_list)) = 4		endif		if(stringmatch(stringfromlist(var_index,str_list),"*Black_928*") == 1)			modifygraph lstyle($stringfromlist(var_index,str_list)) = 0		endif		if(stringmatch(stringfromlist(var_index,str_list),"*Blue_926*") == 1)			modifygraph lstyle($stringfromlist(var_index,str_list)) = 3		endif		if(stringmatch(stringfromlist(var_index,str_list),"*Green_927*") == 1)			modifygraph lstyle($stringfromlist(var_index,str_list)) = 8		endif		if(stringmatch(stringfromlist(var_index,str_list),"*SAL*") == 1)			modifygraph rgb($stringfromlist(var_index,str_list)) = (0,0,0)		endif		if(stringmatch(stringfromlist(var_index,str_list),"*PSEM*") == 1)			modifygraph rgb($stringfromlist(var_index,str_list)) = (2,39321,1)		endif		if(stringmatch(stringfromlist(var_index,str_list),"*PSEM_CNO*") == 1)			modifygraph rgb($stringfromlist(var_index,str_list)) = (65535,0,0)		endif	endif	var_index+=1while(var_index<itemsinlist(str_list))//setaxis bottom -0.1,3.1setaxis bottom -0.1,4.6//modifygraph userticks(bottom)={x_pos_RF,x_label_RF}modifygraph userticks(bottom)={x_pos_GOL,x_label_GOL}modifygraph gfrelsize = 5modifygraph lsize = 1.5setdrawenv fstyle= 1drawtext 0.75,0,"SALINE"setdrawenv fstyle= 1setdrawenv textrgb= (2,39321,1)drawtext 0.75,0.05,"PSEM"setdrawenv fstyle= 1setdrawenv textrgb= (65535,0,0)drawtext 0.75,0.1,"PSEM + CNO"setdrawenv linethick= 1.50setdrawenv linefgc= (34952,34952,34952)drawline 0.75,0.125,0.85,0.125setdrawenv fstyle= 1setdrawenv textrgb= (34952,34952,34952)drawtext 0.9,0.15,"#928"setdrawenv linethick= 1.50setdrawenv linefgc= (34952,34952,34952)setdrawenv dash= 3drawline 0.75,0.175,0.85,0.175setdrawenv fstyle= 1setdrawenv textrgb= (34952,34952,34952)drawtext 0.9,0.2,"#926"setdrawenv linethick= 1.50setdrawenv linefgc= (34952,34952,34952)setdrawenv dash= 8drawline 0.75,0.225,0.85,0.225setdrawenv fstyle= 1setdrawenv textrgb= (34952,34952,34952)drawtext 0.9,0.25,"#927"setdrawenv arrow= 1setdrawenv linethick= 1.50setdrawenv linefgc= (34952,34952,34952)//drawline 0.185,1.175,0.185,1.075drawline -0.075,1.175,-0.075,1.075setdrawenv fstyle= 1setdrawenv textrgb= (34952,34952,34952)//drawtext 0.125,1.25,"drug"drawtext -0.1275,1.25,"drug"movewindow var_left_g,var_top_g,var_right_g,var_bottom_gresumeupdateend