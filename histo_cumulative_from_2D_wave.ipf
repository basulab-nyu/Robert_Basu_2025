#pragma rtGlobals=1#include <Waves Average>macro histo_cumulative_from_2D_waves()phisto_cumulative_from_2D_waves()proc phisto_cumulative_from_2D_waves(str_input,str_binstart,str_binwidth,str_binnum)string str_inputprompt str_input "input waves ?"string str_binstartprompt str_binstart "bin start ?"string str_binwidthprompt str_binwidth "bin width ?"string str_binnumprompt str_binnum "bin number ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_index0variable var_indexvar_index0 = 0silent 1pauseupdatedo	var_index = 0	display	movewindow 0,45,350,350	do		if(strlen(getdimlabel($stringfromlist(var_index0,str_list),1,var_index)) == 0)			histogram/b={str2num(str_binstart),str2num(str_binwidth),str2num(str_binnum)}/c/cum/p/rmd=[][var_index,var_index]/dest=$(stringfromlist(var_index0,str_list)+"_"+num2str(var_index)+"_pdf") $stringfromlist(var_index0,str_list)			appendtograph $(stringfromlist(var_index0,str_list)+"_"+num2str(var_index)+"_pdf")		else			histogram/b={str2num(str_binstart),str2num(str_binwidth),str2num(str_binnum)}/c/cum/p/rmd=[][var_index,var_index]/dest=$(stringfromlist(var_index0,str_list)+"_"+getdimlabel($stringfromlist(var_index0,str_list),1,var_index)+"_pdf") $stringfromlist(var_index0,str_list)			appendtograph $(stringfromlist(var_index0,str_list)+"_"+getdimlabel($stringfromlist(var_index0,str_list),1,var_index)+"_pdf")		endif		var_index+=1	while(var_index<dimsize($stringfromlist(var_index0,str_list),1))	var_index0+=1while(var_index0<itemsinlist(str_list))resumeupdateend