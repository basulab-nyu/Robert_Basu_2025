#pragma rtGlobals=1Macro redimension_2_down_1()predimension_2_down_1()proc predimension_2_down_1(str_input)string str_inputprompt str_input "input waves ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print "\ninput waves:"print replacestring(";",str_list,"\r")print "\nredimensioned waves:"silent 1pauseupdatevariable/g var_index = 0do	if(dimsize($stringfromlist(var_index,str_list),1) == 1)		redimension/n=-1 $stringfromlist(var_index,str_list)		print stringfromlist(var_index,str_list)	endif	var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend