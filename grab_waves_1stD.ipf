#pragma rtGlobals=1Macro grab_waves_1stD()pgrab_waves_1stD()proc pgrab_waves_1stD(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output wave ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print "\ninput waves:"print replacestring(";",str_list,"\r")silent 1pauseupdatevariable/g var_max_length = 0variable/g var_index = 0do	var_max_length = max(var_max_length,dimsize($stringfromlist(var_index,str_list),0))	var_index+=1while(var_index<itemsinlist(str_list))make/o/n=(itemsinlist(str_list),var_max_length) $str_output = nanvariable/g var_index = 0do	duplicate/o/r=[][0] $stringfromlist(var_index,str_list) tempwave	$str_output[var_index][0,dimsize(tempwave,0)-1] = tempwave[q]	setdimlabel 0,var_index,$stringfromlist(var_index,str_list),$str_output	killwaves/z tempwave	var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend