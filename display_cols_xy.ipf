#pragma rtglobals=1macro display_cols_xy()pdisplay_cols_xy()proc pdisplay_cols_xy(str_input,str_y,str_x)string str_inputprompt str_input "input ?"string str_yprompt str_y "y col ?"string str_xprompt str_x "x col ?"if(strlen(str_input) == 0)	str_input = "*"endifstring str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_index = 0pauseupdatedisplaymovewindow 405,38,755.25,353.25do		duplicate/o/r=[][finddimlabel($stringfromlist(var_index,str_list),1,str_y)] $stringfromlist(var_index,str_list),$(stringfromlist(var_index,str_list)+"_"+str_y)	duplicate/o/r=[][finddimlabel($stringfromlist(var_index,str_list),1,str_x)] $stringfromlist(var_index,str_list),$(stringfromlist(var_index,str_list)+"_"+str_x)	appendtograph $(stringfromlist(var_index,str_list)+"_"+str_y) vs $(stringfromlist(var_index,str_list)+"_"+str_x)	//appendtograph $stringfromlist(var_index,str_list)[][%$str_y] vs $stringfromlist(var_index,str_list)[][%$str_x]	var_index+=1while(var_index<itemsinlist(str_list))modifygraph rgb=(0,0,0)resumeupdateend