#pragma rtGlobals=1Macro kill_duplicate_waves_from_match()pkill_duplicate_waves_from_match()proc pkill_duplicate_waves_from_match(str_input,str_target,str_match)string str_inputprompt str_input "input waves ?"string str_targetprompt str_target "target string ?"string str_matchprompt str_match "match string ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)string/g str_kill = replacestring(str_target,str_list,str_match)print "input waves:"print replacestring(";",str_list,"\r")print "matching waves:"variable/g var_index = 0do	if(exists(stringfromlist(var_index,str_kill)) == 1)		print stringfromlist(var_index,str_kill)		killwaves/z $stringfromlist(var_index,str_kill)	endif	var_index+=1while(var_index<itemsinlist(str_kill))