#pragma rtglobals=1macro plot_1D()pplot_1D()proc pplot_1D(str_input)string str_inputprompt str_input "input?"variable var_left_g = 370.5variable var_top_g = 38variable var_right_g = 570.5variable var_bottom_g = 343.25string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_index = 0pauseupdatedisplaymovewindow var_left_g,var_top_g,var_right_g,var_bottom_gdo	appendboxplot $stringfromlist(var_index,str_list)//	modifyboxplot trace=$stringfromlist(var_index,str_list),markers={19,19,19},jitter=1,notched,markerSizes={2,2,2},boxwidth=20	modifyboxplot trace=$stringfromlist(var_index,str_list),markers={19,19,19},jitter=1,markerSizes={2,2,2},boxwidth=20	modifygraph offset($stringfromlist(var_index,str_list)) = {var_index,0}	var_index+=1while(var_index<itemsinlist(str_list))setaxis bottom -0.5,itemsinlist(str_list)-0.5setaxis/a/n=1 leftmodifygraph gfrelsize=6,rgb=(0,0,0)modifygraph userticks(bottom)={xticks,xlbls}resumeupdateend