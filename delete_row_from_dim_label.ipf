#pragma rtGlobals=1Macro delete_row_from_dim_label()pdelete_row_from_dim_label()proc pdelete_row_from_dim_label(str_input,str_lbl)string str_inputprompt str_input "input waves ?"string str_lblprompt str_lbl "target label ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_index variable var_index1variable var_lblvar_index = 0do	var_lbl = finddimlabel($stringfromlist(var_index,str_list),0,str_lbl)	if(var_lbl>=0)		print getdimlabel($stringfromlist(var_index,str_list),0,var_lbl)+" match "+str_lbl+" at "+num2str(var_lbl)+" in "+stringfromlist(var_index,str_list)		make/o/t/n=(dimsize($stringfromlist(var_index,str_list),0)) temp = getdimlabel($stringfromlist(var_index,str_list),0,p)		deletepoints/m=0 var_lbl,1,$stringfromlist(var_index,str_list)		deletepoints/m=0 var_lbl,1,temp		var_index1 = 0		do			setdimlabel 0,var_index1,$(temp[var_index1]),$stringfromlist(var_index,str_list)			var_index1+=1		while(var_index1<dimsize($stringfromlist(var_index,str_list),0))	endif	var_index+=1while(var_index<itemsinlist(str_list))end