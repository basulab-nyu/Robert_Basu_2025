#pragma rtGlobals=1macro display_matrix_xy()pdisplay_matrix_xy()proc pdisplay_matrix_xy(str_input1,str_input2)string str_input1prompt str_input1 "input waves #1 ?"string str_input2prompt str_input2 "input waves #2 ?"string str_list1 = sortlist(wavelist(str_input1,";",""),";",16)string str_list2 = sortlist(wavelist(str_input2,";",""),";",16)print replacestring(";",str_list1,"\r")print replacestring(";",str_list2,"\r")pauseupdatedisplaymovewindow 0,45,350,350variable var_indexvariable var_index1var_index = 0do	var_index1 = 0	do		appendtograph $stringfromlist(var_index,str_list1)[][var_index1] vs $stringfromlist(var_index,str_list2)[][var_index1]		var_index1+=1	while(var_index1<dimsize($stringfromlist(var_index,str_list1),1))	var_index+=1while(var_index<itemsinlist(str_list1))resumeupdateend