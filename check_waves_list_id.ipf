#pragma rtGlobals=1Macro check_waves_list_id()pcheck_waves_list_id()proc pcheck_waves_list_id(str_input)string str_inputprompt str_input "root path ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)string str_substring str_groupvariable var_index = 0do	str_sub = stringfromlist(var_index,str_list)[strsearch(str_input,"*",0),inf]	str_group = sortlist(wavelist("*"+str_sub,";",""),";",16)	make/o/n=(itemsinlist(str_group))/t $str_sub	$str_sub = stringfromlist(p,str_group)	var_index+=1while(var_index<itemsinlist(str_list))end