#pragma rtGlobals=1macro Find_Label_Index()pFind_Label_Index()proc pFind_Label_Index(str_input,str_label,str_output)string str_inputprompt str_input "input waves ?"string str_labelprompt str_label "label ?"string str_outputprompt str_output "output wave ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)make/o/n=(itemsinlist(str_list)) $str_outputvariable/g var_index = 0do	$str_output[var_index] = finddimlabel($stringfromlist(var_index,str_list),0,str_label)	setdimlabel 0,var_index,$nameofwave($stringfromlist(var_index,str_list)),$str_output	var_index+=1while(var_index<itemsinlist(str_list))end