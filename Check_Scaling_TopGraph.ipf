#pragma rtGlobals=1Macro Check_Scaling_TopGraph()pCST()proc pCST()string str_list = tracenamelist("",";",1)string str_list_sort = sortlist (str_list,";",16)string str_list_sort_input_print = tracenamelist("","\r",1)string str_list_sort_input_print_2 = sortlist (str_list_sort_input_print,"\r",16)print str_list_sort_input_print_2variable/g var_index = 0string str_wavesvariable/g var_total = itemsinlist(str_list_sort)make/o/n=(var_total,6) wCheckScaledosilent 1pauseupdate	str_waves = stringfromlist(var_index,str_list_sort)	if (strlen(str_waves) == 0)		break	endif	if (var_index == 0)		wCheckScale[var_index][0] = dimoffset($str_waves,0)		wCheckScale[var_index][1] = dimdelta($str_waves,0)		wCheckScale[var_index][2] = dimsize($str_waves,0)		wCheckScale[var_index][3] = dimoffset($str_waves,1)		wCheckScale[var_index][4] = dimdelta($str_waves,1)		wCheckScale[var_index][5] = dimsize($str_waves,1)		setdimlabel 0,var_index,$str_waves,wCheckScale		setdimlabel 1,0,X_offset,wCheckScale		setdimlabel 1,1,X_delta,wCheckScale		setdimlabel 1,2,X_size,wCheckScale		setdimlabel 1,3,Y_offset,wCheckScale		setdimlabel 1,4,Y_delta,wCheckScale		setdimlabel 1,5,Y_size,wCheckScale	else		wCheckScale[var_index][0] = dimoffset($str_waves,0)		wCheckScale[var_index][1] = dimdelta($str_waves,0)		wCheckScale[var_index][2] = dimsize($str_waves,0)		wCheckScale[var_index][3] = dimoffset($str_waves,1)		wCheckScale[var_index][4] = dimdelta($str_waves,1)		wCheckScale[var_index][5] = dimsize($str_waves,1)		setdimlabel 0,var_index,$str_waves,wCheckScale		setdimlabel 1,0,X_offset,wCheckScale		setdimlabel 1,1,X_delta,wCheckScale		setdimlabel 1,2,X_size,wCheckScale		setdimlabel 1,3,Y_offset,wCheckScale		setdimlabel 1,4,Y_delta,wCheckScale		setdimlabel 1,5,Y_size,wCheckScale	endif	var_index += 1while(1)edit wCheckScale.ldresumeupdateend