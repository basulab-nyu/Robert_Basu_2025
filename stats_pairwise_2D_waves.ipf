#pragma rtGlobals=1macro stats_pairwise_2D_waves()pstats_pairwise_2D_waves()proc pstats_pairwise_2D_waves(str_input,str_dim,str_output)string str_inputprompt str_input "input waves (2D) ?"string str_dimprompt str_dim "groups dimension ?"string str_outputprompt str_output "output wave ?"if(strlen(str_dim) == 0)	str_dim = "0"endifstring str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_indexvariable var_index1variable var_index2make/o/n=(0,2) $str_outputsetdimlabel 1,0,paired_t_test,$str_outputsetdimlabel 1,1,wilcoxon_signed_rank_test,$str_outputvar_index = 0do	duplicate/o $stringfromlist(var_index,str_list) temp	if(str2num(str_dim) == 0)		matrixtranspose temp	endif	var_index1 = 0	do		var_index2 = var_index1+1		do			duplicate/o/r=[][var_index1] temp temp1			duplicate/o/r=[][var_index2] temp temp2			redimension/n=-1 temp1			redimension/n=-1 temp2			killwaves/z w_statsttest			statsttest/q/pair temp1,temp2			redimension/n=(dimsize($str_output,0)+1,2) $str_output			$str_output[dimsize($str_output,0)-1][0] = w_statsttest[%P]			killwaves/z w_wilcoxontest			statswilcoxonranktest/q/wsrt temp1,temp2			$str_output[dimsize($str_output,0)-1][1] = w_wilcoxontest[%P_two_tail]			setdimlabel 0,dimsize($str_output,0)-1,$(stringfromlist(var_index,str_list)+"___"+getdimlabel(temp,1,var_index1)+"_vs_"+getdimlabel(temp,1,var_index2)),$str_output			var_index2+=1		while(var_index2<dimsize(temp,1))		var_index1+=1	while(var_index1<dimsize(temp,1)-1)	var_index+=1while(var_index<itemsinlist(str_list))killwaves/z temp,temp1,temp2,w_statsttest,w_wilcoxontestend