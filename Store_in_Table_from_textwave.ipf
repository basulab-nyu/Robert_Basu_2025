#pragma rtGlobals=1Macro Store_in_Table_from_textwave()pStore_in_Table_from_textwave()proc pStore_in_Table_from_textwave(str_input,str_selection)string str_inputprompt str_input "input reference (textwave) ?"string str_selectionprompt str_selection "data only (0), +index (1), +label (2), label only (3) ?"if(strlen(str_selection) == 0)	variable/g var_selection = 0else	variable/g var_selection = str2num(str_selection)endifif(numpnts($str_input)>0)	edit	variable/g var_index = 0	do		string/g str_temp = $str_input[var_index]		if(var_selection == 0)			appendtotable $str_temp		endif		if(var_selection == 1)			appendtotable $str_temp).id		endif		if(var_selection == 2)			appendtotable $str_temp.ld		endif		if(var_selection == 3)			appendtotable $str_temp.l		endif		var_index+=1	while(var_index<numpnts($str_input))endif