#pragma rtGlobals=1Macro comp_threshold_nan()pcomp_threshold_nan()proc pcomp_threshold_nan(str_input,str_lbl)string str_inputprompt str_input "input waves ?"string str_lblprompt str_lbl "target label ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_index variable var_index1variable var_lblvar_index = 0do	var_lbl = finddimlabel($stringfromlist(var_index,str_list),1,str_lbl)	if(var_lbl>=0)		print getdimlabel($stringfromlist(var_index,str_list),1,var_lbl)+" match "+str_lbl+" at "+num2str(var_lbl)+" in "+stringfromlist(var_index,str_list)		make/o/t/n=(dimsize($stringfromlist(var_index,str_list),1)) temp = getdimlabel($stringfromlist(var_index,str_list),1,p)		deletepoints/m=1 var_lbl,1,$stringfromlist(var_index,str_list)		deletepoints/m=0 var_lbl,1,temp		var_index1 = 0		do			setdimlabel 1,var_index1,$(temp[var_index1]),$stringfromlist(var_index,str_list)			var_index1+=1		while(var_index1<dimsize($stringfromlist(var_index,str_list),1))	endif	var_index+=1while(var_index<itemsinlist(str_list))end