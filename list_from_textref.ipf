#pragma rtGlobals=1		// Use modern global access method.#include <Waves Average>macro list_from_textref()plist_from_textref()proc plist_from_textref(str_input)string str_inputprompt str_input "input textwave ?"if(exists("str_ref") == 2)	killstrings/z str_refendifstring/g str_ref = ""silent 1pauseupdatevariable/g var_index = 0do	str_ref = str_ref+sortlist(wavelist($str_input[var_index],";",""),";",16)	var_index+=1while(var_index<numpnts($str_input))if(itemsinlist(str_ref)>0)	display	edit	variable/g var_index = 0	do		appendtograph $stringfromlist(var_index,str_ref)		appendtotable $stringfromlist(var_index,str_ref).ld		var_index+=1	while(var_index<itemsinlist(str_ref))endifmodifygraph rgb=(0,0,0)resumeupdateprint replacestring(";",str_ref,"\r")end