#pragma rtGlobals=1Macro Set_NaN_from_Wave()pSet_NaN_from_Wave()proc pSet_NaN_from_Wave(str_input1,str_input2)string str_input1prompt str_input1 "source waves ?"string str_input2prompt str_input2 "target waves ?"string str_list1 = sortlist(wavelist(str_input1,";",""),";",16)print sortlist(wavelist(str_input1,"\r",""),"\r",16)string str_list2 = sortlist(wavelist(str_input2,";",""),";",16)print sortlist(wavelist(str_input2,"\r",""),"\r",16)variable var_indexvariable var_index2variable var_index3silent 1pauseupdatevar_index = 0do	var_index2 = 0	do		var_index3 = 0		do			if(numtype($stringfromlist(var_index,str_list1)[var_index2][var_index3]) == 2)				$stringfromlist(var_index,str_list2)[var_index2][var_index3] = nan			endif			var_index3+=1		while(var_index3<dimsize($stringfromlist(var_index,str_list2),1))		var_index2+=1	while(var_index2<dimsize($stringfromlist(var_index,str_list2),0))	var_index+=1while(var_index<itemsinlist(str_list2))resumeupdateend