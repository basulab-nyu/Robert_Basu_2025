#pragma rtglobals=1macro display_xy()pdisplay_xy()proc pdisplay_xy(str_y,str_x)string str_yprompt str_y "y input?"string str_xprompt str_x "x input?"string str_list_y = sortlist(wavelist(str_y,";",""),";",16)string str_list_x = sortlist(wavelist(str_x,";",""),";",16)variable var_indexif(itemsinlist(str_list_y) == itemsinlist(str_list_x))	pauseupdate	display	movewindow 405,38,755.25,353.25	var_index = 0	do			appendtograph $stringfromlist(var_index,str_list_y) vs $stringfromlist(var_index,str_list_x)		print stringfromlist(var_index,str_list_y)+" vs "+stringfromlist(var_index,str_list_x)		var_index+=1	while(var_index<itemsinlist(str_list_y))	resumeupdateelse	print "x-y mismatch!"	print "y data:"	print replacestring(";",str_list_y,"\r")	print "x data:"	print replacestring(";",str_list_x,"\r")endifend