#pragma rtGlobals=1Macro Average_Cursor_TopGraph()pAverage_Cursor_TopGraph()proc pAverage_Cursor_TopGraph(str_output)string str_outputprompt str_output "output wave ?"string str_list = sortlist(tracenamelist("",";",1),";",16)print replacestring(";",str_list,"\r")make/o/n=2 sortwave = nansortwave[0] = xcsr(A)sortwave[1] = xcsr(B)sort sortwave, sortwavemake/o/n=(itemsinlist(str_list)) $str_output = nanvariable/g var_index = 0do	wavestats/q/r=(sortwave[0],sortwave[1]) $stringfromlist(var_index,str_list)	$str_output[var_index] = v_avg	setdimlabel 0,var_index,$stringfromlist(var_index,str_list),$str_output 	var_index += 1while(var_index<itemsinlist(str_list))killwaves/z sortwaveend