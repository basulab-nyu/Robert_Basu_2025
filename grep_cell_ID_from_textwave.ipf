#pragma rtGlobals=1Macro grep_cell_ID_from_textwave()pgrep_cell_ID_from_textwave()proc pgrep_cell_ID_from_textwave(str_input,str_output)string str_inputprompt str_input "input wave (textwave) ?"string str_outputprompt str_output "str_output wave ?"duplicate/o $str_input, $str_output$str_output = ""variable/g var_index = 0do	killstrings/z str_temp	string/g str_temp	splitstring/e=("([[:digit:]]{6}+[[:alpha:]]{1}+[[:digit:]]{1,2})") $str_input[var_index],str_temp	$str_output[var_index] = str_temp	var_index+=1while(var_index<numpnts($str_input))end