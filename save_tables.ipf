#pragma rtGlobals=1macro save_tables()psave_tables()proc psave_tables(str_input,str_path)string str_inputprompt str_input "input waves ?"string str_pathprompt str_path "save path ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")newpath/o/q str_symbpath, str_pathvariable/g var_index = 0do	string/g str_wave = stringfromlist(var_index,str_list)	edit/n=$("temp_table") $str_wave.ld	savetablecopy/p=str_symbpath/o/w=$("temp_table")/t=2 as stringfromlist(var_index,str_list)+".csv"	killwindow/z $("temp_table")	var_index+=1while(var_index < itemsinlist(str_list))