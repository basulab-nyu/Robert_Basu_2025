#pragma rtglobals=1macro move_data()pmove_data()proc pmove_data(str_input,str_folder)string str_inputprompt str_input "input?"string str_folderprompt str_folder "folder?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")if(datafolderexists(str_folder)==0)	newdatafolder $str_folderendifvariable var_index = 0pauseupdate		do	movewave $stringfromlist(var_index,str_list), root:$(str_folder):	var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend