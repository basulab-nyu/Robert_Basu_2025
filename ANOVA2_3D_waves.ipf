#pragma rtGlobals=1macro ANOVA2_3D_waves()pANOVA2_3D_waves()proc pANOVA2_3D_waves(str_input)string str_inputprompt str_input "input waves ?"string str_listvariable var_indexstr_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")silent 1pauseupdatevar_index = 0do	killwaves/z m_anova2results	statsanova2test/modl=1 $stringfromlist(var_index,str_list)	make/o/n=3 $(stringfromlist(var_index,str_list)+"_anova2") = nan	$(stringfromlist(var_index,str_list)+"_anova2")[0] = 1-statsfcdf(m_anova2results[%Factor_A][%F],m_anova2results[%Factor_A][%DF],m_anova2results[%Error_within_cells][%DF])	$(stringfromlist(var_index,str_list)+"_anova2")[1] = 1-statsfcdf(m_anova2results[%Factor_B][%F],m_anova2results[%Factor_B][%DF],m_anova2results[%Error_within_cells][%DF])	$(stringfromlist(var_index,str_list)+"_anova2")[2] = 1-statsfcdf(m_anova2results[%A_x_B][%F],m_anova2results[%A_x_B][%DF],m_anova2results[%Error_within_cells][%DF])	setdimlabel 0,0,factor_A_p_value,$(stringfromlist(var_index,str_list)+"_anova2")	setdimlabel 0,1,factor_B_p_value,$(stringfromlist(var_index,str_list)+"_anova2")	setdimlabel 0,2,factor_AxB_p_value,$(stringfromlist(var_index,str_list)+"_anova2")	edit $(stringfromlist(var_index,str_list)+"_anova2").ld	var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend