#pragma rtGlobals=1macro Prepend_String_Serial()pPrepend_String_Serial()proc pPrepend_String_Serial(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output string ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_indexvar_index = 0do	rename $stringfromlist(var_index,str_list),$(str_output+"_"+stringfromlist(var_index,str_list))	var_index+=1while(var_index<itemsinlist(str_list))end