#pragma rtGlobals=1Macro add_string_to_label()padd_string_to_label()proc padd_string_to_label(str_input,str_add,str_dim)string str_inputprompt str_input "input waves ?"string str_addprompt str_add "string to add ?"string str_dimprompt str_dim "label dimension ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")if(strlen(str_dim) == 0)	variable/g var_dim = 0else	variable/g var_dim = str2num(str_dim)endifvariable/g var_index = 0do	variable/g var_labels = 0	do		string/g str_addlabel = getdimlabel($stringfromlist(var_index,str_list),var_dim,var_labels)+"_"+str_add		setdimlabel var_dim,var_labels,$str_addlabel,$stringfromlist(var_index,str_list)		var_labels+=1	while(var_labels<dimsize($stringfromlist(var_index,str_list),var_dim))	var_index+=1while(var_index<itemsinlist(str_list))end