#pragma rtGlobals=1macro save_graphs()psave_graphs()proc psave_graphs(str_target,str_path)string str_targetprompt str_target "target waves ?"string str_pathprompt str_path "save path ?"string/g str_match = "*"+str_targetnewpath/o/q str_symbpath, str_pathstring/g str_graph_list = sortlist(winlist("*",";","WIN:1"),";",16)variable/g var_index = 0do	getwindow $stringfromlist(var_index,str_graph_list), wavelist	variable/g var_scroll = 0	do		if(stringmatch(w_wavelist[var_scroll][0],str_match) == 1)		//if(stringmatch(w_wavelist[var_scroll][0],"*_SAL_avg") == 1)			string/g str_name = removeending(w_wavelist[var_scroll][0],str_target)			//string/g str_name = removeending(w_wavelist[var_scroll][0],"_SAL_avg")			print stringfromlist(var_index,str_graph_list)			print str_name		endif		var_scroll+=1	while(var_scroll<dimsize(w_wavelist,0))	savepict/e=-6/b=72/p=str_symbpath/o/win=$stringfromlist(var_index,str_graph_list) as str_name+".jpg"	var_index+=1while(var_index<itemsinlist(str_graph_list))