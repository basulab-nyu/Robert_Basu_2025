#pragma rtglobals=1macro get_data()pget_data()proc pget_data(str_input)string str_inputprompt str_input "input?"string str_liststring str_list1variable var_indexvariable var_index1var_index = 0pauseupdatedo	str_list = addlistitem(getindexedobjname("",4,var_index),str_list,";",inf)	var_index+=1while(var_index<countobjects("",4))var_index = 0do	setdatafolder(stringfromlist(var_index,str_list))	print "\r"+stringfromlist(var_index,str_list)	str_list1 = sortlist(wavelist(str_input,";",""),";",16)	if(itemsinlist(str_list1)>0)		print replacestring(";",str_list1,"\r")		var_index1 = 0		do			duplicate/o $stringfromlist(var_index1,str_list1),$(stringfromlist(var_index1,str_list1)+"_copy")			movewave $(stringfromlist(var_index1,str_list1)+"_copy"), root:$stringfromlist(var_index1,str_list1)			var_index1+=1		while(var_index1<itemsinlist(str_list1))	endif	setdatafolder root:	var_index+=1while(var_index<itemsinlist(str_list))end