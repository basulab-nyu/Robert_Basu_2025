#pragma rtGlobals=1Macro append_to_graph()pappend_to_graph()proc pappend_to_graph(str_input,str_ax)string str_inputprompt str_input "input waves ?"string str_axprompt str_ax "target axis (0 = left [default], 1 = right) ?"if(strlen(str_ax) == 0)	str_ax = "0"endifstring str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")silent 1pauseupdatevariable var_index = 0do	if(str2num(str_ax) == 0)		appendtograph $stringfromlist(var_index,str_list)	else		appendtograph/r $stringfromlist(var_index,str_list)	endif	var_index+=1while(var_index<itemsinlist(str_list))resumeupdate