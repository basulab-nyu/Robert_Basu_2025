#pragma rtGlobals=1Macro grab_waves_1stD_indiv()pgrab_waves_1stD_indiv()proc pgrab_waves_1stD_indiv(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output waves ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")silent 1pauseupdatevariable/g var_index = 0do	if(wavetype($stringfromlist(var_index,str_list),1) == 1)		duplicate/o/r=[][0] $stringfromlist(var_index,str_list) $(stringfromlist(var_index,str_list)+"_"+str_output)		redimension/n=-1 $(stringfromlist(var_index,str_list)+"_"+str_output)	endif	var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend