#pragma rtGlobals=1	macro Excise_String()pExcise_String()proc pExcise_String(str_input,str_start,str_length)string str_inputprompt str_input "input waves ?"string str_startprompt str_start "excision start ?"string str_lengthprompt str_length "excision length ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)variable/g var_start = str2num(str_start)variable/g var_length = str2num(str_length)variable/g var_index = 0dosilent 1	print stringfromlist(var_index,str_list)	print nameofwave($stringfromlist(var_index,str_list))[0,var_start]+nameofwave($stringfromlist(var_index,str_list))[var_start+var_length,inf]	duplicate/o $(stringfromlist(var_index,str_list)) $((nameofwave($stringfromlist(var_index,str_list)))[0,var_start]+(nameofwave($stringfromlist(var_index,str_list)))[var_start+var_length,inf])var_index += 1while(var_index<itemsinlist(str_list))end