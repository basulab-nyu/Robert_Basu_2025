#pragma rtGlobals=1Macro append_string_textwaves()pappend_string_textwaves()proc pappend_string_textwaves(str_input,str_target,str_output)string str_inputprompt str_input "input waves ?"string str_targetprompt str_target "target string ?"string str_outputprompt str_output "output string ?"if(strlen(str_input) == 0)	str_input = "control*lbl"endifstring str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_indexvar_index = 0do	$stringfromlist(var_index,str_list) = replacestring(str_target,$stringfromlist(var_index,str_list)[p][q],str_target+"_"+str_output)	var_index+=1while(var_index<itemsinlist(str_list))end