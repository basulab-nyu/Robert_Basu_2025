#pragma rtGlobals=1Macro display_waves_from_textref()pdisplay_waves_from_textref()proc pdisplay_waves_from_textref(str_input)string str_inputprompt str_input "input lists (textwaves) ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)string/g str_graph_list = sortlist(winlist("*",";","WIN:1"),";",16)string/g str_table_list = sortlist(winlist("*",";","WIN:2"),";",16)variable/g var_graph_last = itemsinlist(str_graph_list)-1string/g str_graph_last = winname(0,1)variable/g var_table_last = itemsinlist(str_table_list)-1string/g str_table_last = winname(0,2)getwindow $str_graph_last, wsizevariable/g var_left_g = v_left + 5.25variable/g var_top_g = v_top + 22.5variable/g var_right_g = v_right + 5.25variable/g var_bottom_g = v_bottom + 22.5getwindow $str_table_last, wsizevariable/g var_left_t = v_left + 5.25variable/g var_top_t = v_top + 22.5variable/g var_right_t = v_right + 5.25variable/g var_bottom_t = v_bottom + 22.5if(strlen(str_list)>0)	variable/g var_index = 0	do		display		duplicate/o $stringfromlist(var_index,str_list) temptext		variable/g var_scroll = 0		do			if(exists(nameofwave($($stringfromlist(var_index,str_list)[var_scroll]))) == 1)				appendtograph $($stringfromlist(var_index,str_list)[var_scroll])			else				print temptext[var_scroll]+" not found"			endif			var_scroll+=1		while(var_scroll<numpnts($stringfromlist(var_index,str_list)))		killwaves/z temptext		var_index+=1	while(var_index<itemsinlist(str_list))	modifygraph rgb=(0,0,0)	resumeupdate	movewindow var_left_g,var_top_g,var_right_g,var_bottom_g	showinfo	showtools/aendif