#pragma rtGlobals=1Macro textwaves_to_dim_labels()ptextwaves_to_dim_labels()proc ptextwaves_to_dim_labels(str_input,str_txtwave,str_dim)string str_inputprompt str_input "input waves ?"string str_txtwaveprompt str_txtwave "label waves ?"string str_dimprompt str_dim "dimension ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)string str_txt = sortlist(wavelist(str_txtwave,";",""),";",16)print replacestring(";",str_list,"\r")print replacestring(";",str_txt,"\r")variable var_indexvariable var_labelif(itemsinlist(str_list) != itemsinlist(str_txt))	print "input mismatch !"else	var_index = 0	do		if(dimsize($stringfromlist(var_index,str_txt),0) != dimsize($stringfromlist(var_index,str_list),str2num(str_dim)))			print "length mismatch !"		else			print stringfromlist(var_index,str_list)			print stringfromlist(var_index,str_txt)			var_label = 0			do				setdimlabel str2num(str_dim),var_label,$($stringfromlist(var_index,str_txt)[var_label]),$stringfromlist(var_index,str_list)				var_label+=1			while(var_label<dimsize($stringfromlist(var_index,str_txt),0))			var_index+=1		endif	while(var_index<itemsinlist(str_list))endifend