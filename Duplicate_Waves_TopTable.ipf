#pragma rtGlobals=1Macro Duplicate_Waves_TopTable()pDuplicate_Waves_TopTable()proc pDuplicate_Waves_TopTable(str_output)string str_outputprompt str_output "output waves ?"string/g str_list = sortlist(wavelist("*",";","WIN:"),";",16)print replacestring(";",str_list,"\r")variable/g var_index = 0do	duplicate/o $stringfromlist(var_index,str_list) $(stringfromlist(var_index,str_list)+"_"+str_output)	var_index+=1while(var_index<itemsinlist(str_list))end