#pragma rtGlobals=1Macro replace_NaNs_to_zero_matrix_serial_overwrite()preplace_NaNs_to_zero_matrix_serial_overwrite()proc preplace_NaNs_to_zero_matrix_serial_overwrite(str_input)string str_inputprompt str_input "input waves ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable/g var_index = 0do	$stringfromlist(var_index,str_list) = (numtype($stringfromlist(var_index,str_list)) == 2) ? 0 : $stringfromlist(var_index,str_list)	var_index+=1while(var_index<itemsinlist(str_list))end