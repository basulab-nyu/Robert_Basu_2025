#pragma rtGlobals=1		// Use modern global access method.macro Matrix_Div_by_Vector()pMatrix_Div_by_Vector()proc pMatrix_Div_by_Vector(str_input,str_output)string str_inputprompt str_input "input wave ?"string str_outputprompt str_output "output wave ?"duplicate/o $str_input $str_output$str_output = nansumdimension/d=0/dest=tempsum $str_inputvariable/g var_index = 0do	$str_output[][var_index] = $str_input[p][var_index]/tempsum[var_index]	var_index+=1while(var_index<dimsize($str_input,0))end