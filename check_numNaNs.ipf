#pragma rtGlobals=1Macro check_numNaNs()pcheck_numNaNs()proc pcheck_numNaNs(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "str_output wave ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")make/o/n=(itemsinlist(str_list),2) $str_output = nanvariable/g var_index = 0do	wavestats/q $stringfromlist(var_index,str_list)	$str_output[var_index][0] = v_npnts	$str_output[var_index][1] = v_numnans	setdimlabel 0,var_index,$stringfromlist(var_index,str_list),$str_output	var_index+=1while(var_index<itemsinlist(str_list))setdimlabel 1,0,nPnts,$str_outputsetdimlabel 1,1,numNaNs,$str_outputedit $str_output.ldend