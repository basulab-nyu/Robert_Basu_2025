#pragma rtGlobals=1macro Replace_String_Serial_skipNkill()pReplace_String_Serial_skipNkill()proc pReplace_String_Serial_skipNkill(str_input,str_target,str_output)string str_inputprompt str_input "input waves ?"string str_targetprompt str_target "target string ?"string str_outputprompt str_output "output string ?"string str_indexstring str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")silent 1pauseupdatevariable/g var_index = 0do	print stringfromlist(var_index,str_list)	if(exists(replacestring(str_target,stringfromlist(var_index,str_list),str_output)) == 1)		killwaves/z $stringfromlist(var_index,str_list)		print "killed "+stringfromlist(var_index,str_list)	else		rename $stringfromlist(var_index,str_list),$replacestring(str_target,stringfromlist(var_index,str_list),str_output)		print "renamed as "+replacestring(str_target,stringfromlist(var_index,str_list),str_output)	endif	var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend