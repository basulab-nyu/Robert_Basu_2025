#pragma rtGlobals=1Macro Count_Data_Zeros_NaNs_Infs()pCount_Data_Zeros_NaNs_Infs()proc pCount_Data_Zeros_NaNs_Infs(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output wave ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")make/o/n=(itemsinlist(str_list),5) $str_output = nansetdimlabel 1,0,non_zero_data,$str_outputsetdimlabel 1,1,zeros,$str_outputsetdimlabel 1,2,NaNs,$str_outputsetdimlabel 1,3,infs,$str_outputsetdimlabel 1,4,total,$str_outputvariable var_indexvar_index = 0do	duplicate/o $stringfromlist(var_index,str_list) tempwave	wavestats/q tempwave	$str_output[var_index][0] = v_npnts	$str_output[var_index][2] = v_numnans	$str_output[var_index][3] = v_numinfs	$str_output[var_index][4] = dimsize(tempwave,0)	wavetransform/o zapnans tempwave	tempwave = (tempwave[p] == 0) ? NaN : tempwave[p]	wavestats/q tempwave	$str_output[var_index][1] = v_numnans	$str_output[var_index][0] = $str_output[var_index][0] - $str_output[var_index][1]	killwaves/z tempwave	setdimlabel 0,var_index,$stringfromlist(var_index,str_list),$str_output	var_index+=1while(var_index<itemsinlist(str_list))edit $str_output.ld