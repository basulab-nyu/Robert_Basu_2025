#pragma rtGlobals=1macro Append_String_Folders()pASF()proc pASF(str_input,str_output)string str_inputprompt str_input "input folders ?"string str_outputprompt str_output "output string ?"if(exists("str_folder_list")==2)	killstrings str_folder_listendifif(exists("str_folder_list_print")==2)	killstrings str_folder_list_printendifstring/g str_folder_liststring/g str_folder_list_printvariable/g var_index = 0do	str_folder_list = addlistitem(getindexedobjname("", 4, var_index),str_folder_list,";",var_index)	str_folder_list_print = addlistitem(getindexedobjname("", 4, var_index),str_folder_list_print,"\r",var_index)	var_index+=1while(var_index<countobjects("",4))str_folder_list=listmatch(str_folder_list,str_input,";")str_folder_list_print=listmatch(str_folder_list_print,str_input,"\r")print str_folder_list_printvariable/g var_index = 0do	renamedatafolder $stringfromlist(var_index,str_folder_list),$(stringfromlist(var_index,str_folder_list)+"_"+str_output)	var_index+=1while(var_index<itemsinlist(str_folder_list))end