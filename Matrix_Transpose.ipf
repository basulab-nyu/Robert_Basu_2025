#pragma rtGlobals=1#include <Waves Average>macro Matrix_Transpose()pMatrix_Transpose()proc pMatrix_Transpose(str_input)string str_inputprompt str_input "input waves ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")variable var_index = 0do	matrixtranspose $stringfromlist(var_index,str_list)	var_index+=1while(var_index<itemsinlist(str_list))end