#pragma rtGlobals=1Macro Differentiate_waves()pDifferentiate_waves()proc pDifferentiate_waves(str_input)string str_inputprompt str_input "input waves ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print sortlist(wavelist(str_input,"\r",""),"\r",16)variable/g var_index = 0dosilent 1pauseupdate	string/g str_waves = stringfromlist(var_index,str_list)	duplicate/o $str_waves $(nameofwave($str_waves)+"_d")	differentiate $(nameofwave($str_waves)+"_d")var_index+=1while (var_index < itemsinlist(str_list))resumeupdateend