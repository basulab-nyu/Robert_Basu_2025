#pragma rtGlobals=1macro Rename_Folders_from_textwave()pRename_Folders_from_textwave()proc pRename_Folders_from_textwave(str_input)string str_inputprompt str_input "reference textwave ?"killstrings/z str_folder_liststring/g str_folder_listvariable/g var_index = 0do	str_folder_list = addlistitem(getindexedobjname("", 4, var_index),str_folder_list,";",var_index)	var_index+=1while(var_index<countobjects("",4))//print replacestring(";",str_folder_list,"\r")variable/g var_index = 0do	variable/g var_scroll = 0	do		if(grepstring($str_input[var_scroll],stringfromlist(var_index,str_folder_list)) == 1)			//print stringfromlist(var_index,str_folder_list)			//killstrings/z str_temp			//string/g str_temp = $str_input[var_scroll]			//print str_temp			renamedatafolder $stringfromlist(var_index,str_folder_list),$($str_input[var_scroll])			var_scroll = numpnts($str_input)		else			var_scroll+=1		endif	while(var_scroll<numpnts($str_input))	var_index+=1while(var_index<itemsinlist(str_folder_list))end