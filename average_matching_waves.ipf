#pragma rtGlobals=1	macro Average_Matching_Waves()pAvgMWS()proc pAvgMWS(str_input,str_target)string str_inputprompt str_input "input waves ?"string str_targetprompt str_target "target string ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)variable/g var_index = 0do	string/g str_group = sortlist(wavelist(replacestring(str_target,stringfromlist(var_index,str_list),"*"),";",""),";",16)	print sortlist(wavelist(replacestring(str_target,stringfromlist(var_index,str_list),"*"),"\r",""),"\r",16)	//fwaveaverage(str_group,"",0,2,stringfromlist(var_index,str_list)+"_"+str_output,"")	fwaveaverage(str_group,"",3,0,stringfromlist(var_index,str_list)+"_avg",stringfromlist(var_index,str_list)+"_sem")	var_index+=1while(var_index<itemsinlist(str_list))	resumeupdateend