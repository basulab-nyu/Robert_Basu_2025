#pragma rtGlobals=1macro display_fit_matrix_xy()pdisplay_fit_matrix_xy()proc pdisplay_fit_matrix_xy(str_input1,str_input2)string str_input1prompt str_input1 "input waves #1 ?"string str_input2prompt str_input2 "input waves #2 ?"string str_list1 = sortlist(wavelist(str_input1,";",""),";",16)string str_list2 = sortlist(wavelist(str_input2,";",""),";",16)print replacestring(";",str_list1,"\r")print replacestring(";",str_list2,"\r")variable var_indexvariable var_sizevariable var_NaN1variable var_NaN2make/o/n=(itemsinlist(str_list1),2) stats_linreg = nansetdimlabel 1,0,coeff,stats_linregsetdimlabel 1,1,r2,stats_linregpauseupdatedisplaymovewindow 400,45,750,350var_index = 0do	wavestats/q $stringfromlist(var_index,str_list1)	var_NaN1 = v_numnans	wavestats/q $stringfromlist(var_index,str_list2)	var_NaN2 = v_numnans	if(numpnts($stringfromlist(var_index,str_list1)) > var_NaN1 && numpnts($stringfromlist(var_index,str_list2)) > var_NaN2)		var_size = max(dimsize($stringfromlist(var_index,str_list1),0)*dimsize($stringfromlist(var_index,str_list1),1),dimsize($stringfromlist(var_index,str_list2),0)*dimsize($stringfromlist(var_index,str_list2),1))		make/o/n=(var_size) temp1 = nan		make/o/n=(var_size) temp2 = nan		duplicate/o $stringfromlist(var_index,str_list1),base1		duplicate/o $stringfromlist(var_index,str_list2),base2		matrixop/o temp1 = redimension(base1,var_size,1)		matrixop/o temp2 = redimension(base2,var_size,1)		duplicate/o temp1,$(stringfromlist(var_index,str_list1)+"_v")		duplicate/o temp2,$(stringfromlist(var_index,str_list2)+"_v")		appendtograph $(stringfromlist(var_index,str_list1)+"_v") vs $(stringfromlist(var_index,str_list2)+"_v")		modifygraph mode($(stringfromlist(var_index,str_list1)+"_v"))=3,marker($(stringfromlist(var_index,str_list1)+"_v"))=19,msize($(stringfromlist(var_index,str_list1)+"_v"))=1.5,rgb($(stringfromlist(var_index,str_list1)+"_v"))=(0,0,0,32768)		if(stringmatch(stringfromlist(var_index,str_list1)+"_v","*control*") == 1)			modifygraph rgb($(stringfromlist(var_index,str_list1)+"_v"))=(0,0,0,32768)		endif		if(stringmatch(stringfromlist(var_index,str_list1)+"_v","*LECglu*") == 1)			modifygraph rgb($(stringfromlist(var_index,str_list1)+"_v"))=(2,39321,1,32768)		endif		if(stringmatch(stringfromlist(var_index,str_list1)+"_v","*LECgaba*") == 1)			modifygraph rgb($(stringfromlist(var_index,str_list1)+"_v"))=(65535,0,0,32768)		endif		curvefit/q line,$(stringfromlist(var_index,str_list1)+"_v")/x=$(stringfromlist(var_index,str_list2)+"_v")/d		duplicate/o $("fit_"+stringfromlist(var_index,str_list1)+"_v"),$("fit_"+stringfromlist(var_index,str_list1)+"_vs_"+stringfromlist(var_index,str_list2))		removefromgraph $("fit_"+stringfromlist(var_index,str_list1)+"_v")		killwaves/z $("fit_"+stringfromlist(var_index,str_list1)+"_v")		appendtograph $("fit_"+stringfromlist(var_index,str_list1)+"_vs_"+stringfromlist(var_index,str_list2))		if(stringmatch(stringfromlist(var_index,str_list1)+"_v","*control*") == 1)			modifygraph rgb($("fit_"+stringfromlist(var_index,str_list1)+"_vs_"+stringfromlist(var_index,str_list2)))=(0,0,0)		endif		if(stringmatch(stringfromlist(var_index,str_list1)+"_v","*LECglu*") == 1)			modifygraph rgb($("fit_"+stringfromlist(var_index,str_list1)+"_vs_"+stringfromlist(var_index,str_list2)))=(2,39321,1)		endif		if(stringmatch(stringfromlist(var_index,str_list1)+"_v","*LECgaba*") == 1)			modifygraph rgb($("fit_"+stringfromlist(var_index,str_list1)+"_vs_"+stringfromlist(var_index,str_list2)))=(65535,0,0)		endif		statslinearregression/pair/q $(stringfromlist(var_index,str_list1)+"_v"),$(stringfromlist(var_index,str_list2)+"_v")		stats_linreg[var_index][0] = W_StatsLinearRegression[0][%b]		stats_linreg[var_index][1] = W_StatsLinearRegression[0][%r2]		setdimlabel 0,var_index,$(stringfromlist(var_index,str_list1)+"_vs_"+stringfromlist(var_index,str_list2)),stats_linreg	endif	var_index+=1while(var_index<itemsinlist(str_list1))resumeupdate getaxis/q leftsetaxis left max(-1,1.1*v_min),min(1,1.1*v_max)setdrawenv xcoord= bottom,ycoord= left,linefgc= (0,0,0,32768),dash= 3drawline 0,max(-1,1.1*v_min),0,min(1,1.1*v_max)getaxis/q bottomsetaxis bottom max(-1,1.1*v_min),min(1,1.1*v_max)setdrawenv xcoord= bottom,ycoord= left,linefgc= (0,0,0,32768),dash= 3drawline max(-1,1.1*v_min),0,min(1,1.1*v_max),0pauseupdateif(stringmatch(stringfromlist(0,str_list1)+"_v","*_PV_*") == 1)	label left "PV correlation"endifif(stringmatch(stringfromlist(0,str_list1)+"_v","*_TC_*") == 1)	label left "TC correlation"endifif(stringmatch(stringfromlist(0,str_list2)+"_v","*_DI_*") == 1)	label bottom "discrimination index"endifif(stringmatch(stringfromlist(0,str_list2)+"_v","*_PV_*") == 1)	label bottom "PV correlation"endifif(stringmatch(stringfromlist(0,str_list2)+"_v","*_TC_*") == 1)	label bottom "TC correlation"endifif(stringmatch(stringfromlist(0,str_list1)+"_v","*_DI_*") == 1)	label left "discrimination index"endifmodifygraph gfrelsize = 4killwaves/z temp1,temp2,base1,base2,w_coef,w_statslinearregression,w_sigmaresumeupdateend