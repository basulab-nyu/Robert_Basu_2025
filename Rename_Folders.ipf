#pragma rtGlobals=1macro Rename_Folders()pRename_Folders()proc pRename_Folders(str_input,str_output)string str_inputprompt str_input "input string ?"string str_outputprompt str_output "output string ?"string str_listvariable var_indexsilent 1pauseupdatestr_list = ""var_index = 0do	str_list = addlistitem(getindexedobjname("",4,var_index),str_list,";",var_index)	var_index+=1while(var_index<countobjects("",4))str_list = removefromlist("Packages",str_list)var_index = 0do	if(stringmatch(stringfromlist(var_index,str_list),"*"+str_input+"*") == 1)		renamedatafolder $stringfromlist(var_index,str_list), $(replacestring(str_input,stringfromlist(var_index,str_list),str_output))	endif	var_index+=1while(var_index<itemsinlist(str_list))end