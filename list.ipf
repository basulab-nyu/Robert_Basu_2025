control_ad_TC_corr_pair_txt#pragma rtGlobals=1macro list()plist()proc plist(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output wave ?"if(strlen(str_output) == 0)	str_output = "wref"endifstring str_list = sortlist(wavelist(str_input,";",""),";",16)make/o/t/n=(itemsinlist(str_list)) $str_output = stringfromlist(p,str_list)edit $str_outputend