#pragma rtGlobals=1macro matrix_cols_to_vectors()pmatrix_cols_to_vectors()proc pmatrix_cols_to_vectors(str_input)string str_inputprompt str_input "input waves ?"string str_listvariable var_indexvariable var_index2str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")silent 1pauseupdatevar_index = 0do	var_index2 = 0	do		if(strlen(getdimlabel($stringfromlist(var_index,str_list),1,var_index2)) == 0)			duplicate/o/r=[][var_index2] $stringfromlist(var_index,str_list),$(stringfromlist(var_index,str_list)+"_"+num2str(var_index2))			redimension/n=-1 $(stringfromlist(var_index,str_list)+"_"+num2str(var_index2))		else			duplicate/o/r=[][var_index2] $stringfromlist(var_index,str_list),$(stringfromlist(var_index,str_list)+"_"+getdimlabel($stringfromlist(var_index,str_list),1,var_index2))			redimension/n=-1 $(stringfromlist(var_index,str_list)+"_"+getdimlabel($stringfromlist(var_index,str_list),1,var_index2))		endif		var_index2+=1	while(var_index2<dimsize($stringfromlist(var_index,str_list),1))	var_index+=1while(var_index<itemsinlist(str_list))resumeupdateend