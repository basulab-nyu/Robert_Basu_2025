#pragma rtGlobals=1Macro List_Matching_Waves()pList_Matching_Waves()proc pList_Matching_Waves(str_input,str_target,str_match)string str_inputprompt str_input "input waves ?"string str_targetprompt str_target "target string ?"string str_matchprompt str_match "matching string ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print sortlist(wavelist(str_input,"\r",""),"\r",16)string/g str_matchlist = replacestring(str_target,str_list,str_match)print replacestring(";",str_matchlist,"\r")silent 1pauseupdatemake/o/t/n=(itemsinlist(str_list)) w_list_tmake/o/t/n=(itemsinlist(str_list)) w_matchlist_tvariable/g var_index = 0do	w_list_t[var_index] = stringfromlist(var_index,str_list)	w_matchlist_t[var_index] = stringfromlist(var_index,str_matchlist)	var_index+=1while(var_index<itemsinlist(str_list))edit w_list_t,w_matchlist_tresumeupdateend