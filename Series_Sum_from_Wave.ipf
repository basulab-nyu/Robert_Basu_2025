#pragma rtGlobals=1Macro Series_Sum_from_Wave()pSSfW()proc pSSfW(str_input,str_output)string str_inputprompt str_input "input wave ?"string str_outputprompt str_output "output wave ?"duplicate/o $str_input $str_output$str_output = NaNvariable/g var_end = numpnts($str_input)variable/g var_index = 0dosilent 1	if (var_index == 0)		$str_output[0] = $str_input[0]	else		$str_output[var_index] = sum($str_input,0,pnt2x($str_input,var_index))	endifvar_index = var_index + 1while (var_index <= var_end)end