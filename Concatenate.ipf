#pragma rtGlobals=1Macro Concatenate_Waves()CW()proc CW(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output wave ?"string str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")concatenate/np=0/o str_list,$str_outputsilent 1pauseupdatevariable/g var_index = 0do	setdimlabel 0,var_index,$stringfromlist(var_index,str_list),$str_output	var_index += 1while(var_index<itemsinlist(str_list))edit $str_output.ldresumeupdateend