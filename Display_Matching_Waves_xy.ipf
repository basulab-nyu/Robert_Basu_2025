#pragma rtGlobals=1macro Display_Matching_Waves_xy()pDisplay_Matching_Waves_xy()proc pDisplay_Matching_Waves_xy(str_input,str_target,str_match)string str_inputprompt str_input "input waves ?"string str_targetprompt str_target "target string ?"string str_matchprompt str_match "matching string ?"string str_list1 = sortlist(wavelist(str_input,";",""),";",16)string str_list2 = replacestring(str_target,str_list1,str_match)print replacestring(";",str_list1,"\r")print replacestring(";",str_list2,"\r")silent 1pauseupdatedisplaymovewindow 0,45,350,350variable var_index = 0do	appendtograph $stringfromlist(var_index,str_list1) vs $stringfromlist(var_index,str_list2)	var_index+=1while(var_index<itemsinlist(str_list1))resumeupdateend