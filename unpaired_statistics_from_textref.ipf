#pragma rtGlobals=1macro unpaired_statistics_from_textref()punpaired_statistics_from_textref()proc punpaired_statistics_from_textref(str_input,str_output)string str_inputprompt str_input "input waves (textwave) ?"string str_outputprompt str_output "output wave ?"if(strlen(str_input) == 0)	str_input = "wref"endifstring str_liststring str_basestring str_name0string str_name1variable var_indexvariable var_index1variable var_index2make/o/n=(numpnts($str_input),4) $str_output = nansetdimlabel 1,0,Ttest,$str_outputsetdimlabel 1,1,MannWhitney,$str_outputsetdimlabel 1,2,Anova1,$str_outputsetdimlabel 1,3,KruskalWallis,$str_outputvar_index = 0do	str_list = sortlist(wavelist($str_input[var_index],";",""),";",16)	print replacestring(";",str_list,"\r")	if(itemsinlist(str_list) == 2)		killwaves/z w_statsttest		statsttest/q $stringfromlist(0,str_list),$stringfromlist(1,str_list)		$str_output[var_index][0] = w_statsttest[%P]		killwaves/z W_WilcoxonTest		statswilcoxonranktest/q/tail=4 $stringfromlist(0,str_list),$stringfromlist(1,str_list)		$str_output[var_index][1] = W_WilcoxonTest[%P_TwoTail]		str_base = ""		str_base = $str_input[var_index]		do			str_base = str_base[0,strsearch(str_base,"*",0)-1]+str_base[strsearch(str_base,"*",0)+1,inf]		while(grepstring(str_base,"\*") == 1)		str_name0 = replacestring(str_base,stringfromlist(0,str_list),"")		str_name1 = replacestring(str_base,stringfromlist(1,str_list),"")		setdimlabel 0,var_index,$(str_base+"_"+str_name0+"_"+getdimlabel($stringfromlist(0,str_list),0,0)+"_vs_"+str_name1+"_"+getdimlabel($stringfromlist(1,str_list),0,0)),$str_output	else		killwaves/z M_ANOVA1		statsanova1test/q/wstr=str_list		$str_output[var_index][2] = M_ANOVA1[%Total][%P]		killwaves/z W_KWTestResults		statskwtest/q/wstr=str_list		$str_output[var_index][3] = 0.5*(W_KWTestResults[%P_Chi_square_approx]+W_KWTestResults[%P_Wallace_approx])	endif	var_index+=1while(var_index<numpnts($str_input))killwaves/z w_statsttest,W_WilcoxonTest,M_ANOVA1,W_KWTestResultsend