#pragma rtGlobals=1Macro Insert_NaNs_TopTable()pInsert_NaNs_TopTable()proc pInsert_NaNs_TopTable(str_idx,str_nb,str_output)string str_idxprompt str_idx "insertion index ?"string str_nbprompt str_nb "number of rows to insert ?"string str_outputprompt str_output "output waves ?"if(strlen(str_idx)>0 && strlen(str_nb)>0)	variable/g var_idx = str2num(str_idx)	variable/g var_nb = str2num(str_nb)	string/g str_list = sortlist(wavelist("*",";","WIN:"),";",16)	print replacestring(";",str_list,"\r")	variable/g var_index = 0	do		if(strlen(str_output)>0)			getwindow kwtopwin, wsize			variable/g var_left_t = v_left + 5.25			variable/g var_top_t = v_top + 22.5			variable/g var_right_t = v_right + 5.25			variable/g var_bottom_t = v_bottom + 22.5			duplicate/o $stringfromlist(var_index,str_list),$(stringfromlist(var_index,str_list)+"_"+str_output)			insertpoints/m=0/v=nan var_idx,var_nb,$(stringfromlist(var_index,str_list)+"_"+str_output)			if(var_index == 0)				edit $(stringfromlist(var_index,str_list)+"_"+str_output).ld				movewindow var_left_t,var_top_t,var_right_t,var_bottom_t			else				appendtotable $(stringfromlist(var_index,str_list)+"_"+str_output).ld			endif		else			insertpoints/m=0/v=nan var_idx,var_nb,$stringfromlist(var_index,str_list)		endif		var_index+=1	while(var_index<itemsinlist(str_list))endifend