#pragma rtGlobals=1Macro movewaves_from_textwaveref()pmovewaves_from_textwaveref()proc pmovewaves_from_textwaveref(str_input,str_folder)string str_inputprompt str_input "reference textwave [grep] ?"string str_folderprompt str_folder "output folder ?"if(strlen(str_folder) == 0)	str_folder = "move_folder"endifif(datafolderexists(str_folder) == 0)	newdataFolder $str_folderendifsilent 1pauseupdatevariable/g var_index = 0do	string/g str_move_list = greplist(sortlist(wavelist("*",";",""),";",16),$str_input[var_index])	if(strlen(str_move_list)>0)		variable/g var_move = 0		do			movewave $stringfromlist(var_move,str_move_list), root:$(str_folder):			var_move+=1		while(var_move<itemsinlist(str_move_list))	endif	var_index+=1while(var_index<numpnts($str_input))resumeupdate