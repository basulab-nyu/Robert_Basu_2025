#pragma rtGlobals=1macro two_samples_stats_summary()ptwo_samples_stats_summary()proc ptwo_samples_stats_summary(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output waves ?"string str_listvariable var_indexstr_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")make/o/n=(13,itemsinlist(str_list)) $str_output = nansetdimlabel 0,0,avg,$str_outputsetdimlabel 0,1,sem,$str_outputsetdimlabel 0,2,n,$str_outputsetdimlabel 0,3,shapiro_wilk,$str_outputsetdimlabel 0,4,jarque_bera,$str_outputsetdimlabel 0,5,shapiro_wilk_diff,$str_outputsetdimlabel 0,6,jarque_bera_diff,$str_outputsetdimlabel 0,7,homoscedasticity,$str_outputsetdimlabel 0,8,t_test,$str_outputsetdimlabel 0,9,mann_whitney_test,$str_outputsetdimlabel 0,10,paired_t_test,$str_outputsetdimlabel 0,11,wilcoxon_signed_rank_test,$str_outputsetdimlabel 0,12,kolmogorov_smirnov,$str_outputsetdimlabel 1,0,$stringfromlist(0,str_list),$str_outputsetdimlabel 1,1,$stringfromlist(1,str_list),$str_outputvar_index = 0do	wavestats/q $stringfromlist(var_index,str_list)	$str_output[%avg][var_index] = v_avg	$str_output[%sem][var_index] = v_sem	$str_output[%n][var_index] = v_npnts	killvariables/z v_prob	statsshapirowilktest/q/z $stringfromlist(var_index,str_list)	if(v_prob < 0.05)		$str_output[%shapiro_wilk][var_index] = 0	else		$str_output[%shapiro_wilk][var_index] = 1	endif	killwaves/z W_JBResults	statsjbtest/q/z $stringfromlist(var_index,str_list)	if(w_jbresults[%JBStatistic] >= w_jbresults[%Critical])		$str_output[%jarque_bera][var_index] = 0	else		$str_output[%jarque_bera][var_index] = 1	endif	var_index+=1while(var_index<itemsinlist(str_list))if(numpnts($stringfromlist(0,str_list)) == numpnts($stringfromlist(1,str_list)))	duplicate $stringfromlist(0,str_list),tempdiff	tempdiff = $stringfromlist(0,str_list)-$stringfromlist(1,str_list)	killvariables/z v_prob	statsshapirowilktest/q/z tempdiff	if(v_prob < 0.05)		$str_output[%shapiro_wilk_diff][0] = 0	else		$str_output[%shapiro_wilk_diff][0] = 1	endif	killwaves/z W_JBResults	statsjbtest/q/z tempdiff	if(w_jbresults[%JBStatistic] >= w_jbresults[%Critical])		$str_output[%jarque_bera_diff][0] = 0	else		$str_output[%jarque_bera_diff][0] = 1	endif	killwaves/z tempdiffendifkillwaves/z w_statsvariancesteststatsvariancestest/q $stringfromlist(0,str_list),$stringfromlist(1,str_list)if(w_statsvariancestest[%Bartlett_T] >= w_statsvariancestest[%Critical])	$str_output[%homoscedasticity][0] = 0else	$str_output[%homoscedasticity][0] = 1endifkillwaves/z w_statstteststatsttest/q $stringfromlist(0,str_list),$stringfromlist(1,str_list)$str_output[%t_test][0] = w_statsttest[%p]killwaves/z w_wilcoxonteststatswilcoxonranktest/q/tail=4 $stringfromlist(0,str_list),$stringfromlist(1,str_list)$str_output[%mann_whitney_test][0] = w_wilcoxontest[%P_TwoTail]if(numpnts($stringfromlist(0,str_list)) == numpnts($stringfromlist(1,str_list)))	killwaves/z w_statsttest	statsttest/q/pair $stringfromlist(0,str_list),$stringfromlist(1,str_list)	$str_output[%paired_t_test][0] = w_statsttest[%p]	killwaves/z w_wilcoxontest	statswilcoxonranktest/q/wsrt $stringfromlist(0,str_list),$stringfromlist(1,str_list)	$str_output[%wilcoxon_signed_rank_test][0] = w_wilcoxontest[%P_two_tail]endifkillwaves/z w_ksresultsstatskstest/q $stringfromlist(0,str_list),$stringfromlist(1,str_list)$str_output[%kolmogorov_smirnov][0] = (w_ksresults[%$("PValue(Ne)")]+w_ksresults[%$("PValue(Marsaglia)")]+w_ksresults[%$("PValue(B&T)")])/3edit $str_output.ldmovewindow 0,45,350,350killvariables/z v_probkillwaves/z W_JBResultskillwaves/z w_statsvariancestestkillwaves/z w_statsttestkillwaves/z w_wilcoxontestkillwaves/z w_ksresultsend