#pragma rtGlobals=1Macro list_to_table()plist_to_table()proc plist_to_table(str_input,str_output)string str_inputprompt str_input "input waves ?"string str_outputprompt str_output "output name ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")make/t/o/n=(itemsinlist(str_list)) $str_outputvariable/g var_index = 0do	if(wavetype($stringfromlist(var_index,str_list),1) == 1)		$str_output[var_index] = stringfromlist(var_index,str_list)	endif	var_index+=1while(var_index<itemsinlist(str_list))edit $str_output