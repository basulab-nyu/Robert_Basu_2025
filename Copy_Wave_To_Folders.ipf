#pragma rtGlobals=1Macro Copy_Wave_To_Folders()pCopy_Wave_To_Folders()proc pCopy_Wave_To_Folders(str_input)string str_inputprompt str_input "input wave ?"variable/g var_Fcount = countobjects("",4)killstrings/z str_Fliststring/g str_Flistvariable/g var_Fscroll = 0silent 1pauseupdatedo	str_Flist = addlistitem(getindexedobjname("", 4, var_Fscroll),str_Flist,";",var_Fscroll)	var_Fscroll+=1while(var_Fscroll<var_Fcount)variable/g var_Findex = 0do	string/g str_Fbrowse = stringfromlist(var_Findex,str_Flist)	print str_Fbrowse	string/g str_name = str_Fbrowse	duplicate/o $str_input, tempwave	movewave tempwave, root:$(str_Fbrowse):$str_input	var_Findex+=1while(var_Findex<itemsinlist(str_Flist))resumeupdateend