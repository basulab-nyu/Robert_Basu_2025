#pragma rtGlobals=1Macro Ttest_vs_exp_mean()pTtest_vs_exp_mean()proc pTtest_vs_exp_mean(str_input)string str_inputprompt str_input "input waves ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print sortlist(wavelist(str_input,"\r",""),"\r",16)string/g str_name = str_input[0,strsearch(str_input,"*",0)-1]+"pval"make/n=(itemsinlist(str_list)) $str_name = nanvariable/g var_index = 0do	statsttest/mean=0/q $stringfromlist(var_index,str_list)	$str_name[var_index] = W_StatsTTest[%P]	killwaves/z W_StatsTTest	var_index+=1while(var_index<itemsinlist(str_list))end