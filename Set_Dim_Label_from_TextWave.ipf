#pragma rtGlobals=1Macro Set_Dim_Label_from_TextWave()pSet_Dim_Label_from_TextWave()proc pSet_Dim_Label_from_TextWave(str_input,str_txtwave)string str_inputprompt str_input "input waves ?"string str_txtwaveprompt str_txtwave "label wave ?"string/g str_list = sortlist(wavelist(str_input,";",""),";",16)print replacestring(";",str_list,"\r")print $str_txtwavevariable/g var_index = 0do	variable/g var_label = 0	do		setdimlabel 0,var_label,$($(str_txtwave)[var_label]),$stringfromlist(var_index,str_list)		var_label+=1	while(var_label<numpnts($str_txtwave))	var_index+=1while(var_index<itemsinlist(str_list))end